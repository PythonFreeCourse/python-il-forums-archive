<pre><code class="lang-auto">def clean_lines(txt):
    new_list = []
    for line in txt:
        new_list.append(int(line.strip('\n')))
    return new_list


def calculator(num_list):
    total_fuel = 0
    for num in num_list:
        fuel = 0
        while num &gt; 5:
            num = num // 3 - 2
            fuel += num
        total_fuel += fuel
    return total_fuel


with open('day1.txt', 'r') as file:
    numbers = file.readlines()

calculator(clean_lines(numbers))
